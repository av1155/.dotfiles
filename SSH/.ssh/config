# Default settings for all hosts
Host *
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_ed25519
    SetEnv TERM=xterm-256color

# Host specific settings
Host github.com
    User git
    IdentityFile ~/.ssh/id_ed25519

Host pi-server
  HostName 192.168.4.139
  User andreaventi
  IdentityFile ~/.ssh/id_pi
