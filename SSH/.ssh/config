# Default settings for all hosts
Host *
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_ed25519
    SetEnv TERM=xterm-256color

# Host specific settings
Host github.com
    User git
    IdentityFile ~/.ssh/id_ed25519

Host pi-server
  HostName 192.168.10.51
  User andreaventi
    IdentityFile ~/.ssh/id_pi

Host ec2-aws-01
  HostName 3.129.18.41
  User ec2-user
    IdentityFile ~/.ssh/MyAndreaventiKeyPair.pem

